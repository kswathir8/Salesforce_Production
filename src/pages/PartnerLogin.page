        <div class="login-sidebar clearfix">
            <div class="login-box">
                <div class="up-logo">Axcient User Portal</div>
                <apex:form id="login">
                    <apex:pageMessages />
                    <p>Please log in using your Axcient provided portal credentials</p>
                    <div class="login-input">
                        <div class="login-name-block"></div>
                        <!--<input type="text" class="login-name validate[required,custom[email]]" name="user" placeholder="Email" />-->
                        <apex:inputText id="inputUsername" value="{!username}" styleClass="login-name validate[required,custom[email]]" />
                    
                        <div class="clear"></div>
                        <div class="login-password-block"></div>
                        <!--<input type="password" class="login-password validate[required]" name="password" placeholder="Password" />-->
                        <apex:inputSecret id="inputPassword" value="{!password}" styleClass="login-password validate[required]" />
                    </div>
                    <div class="login-submit clearfix">
                        <a class="forgot" href="{!$Page.PartnerForgotPassword}" title="Click here to reset your password">Reset Password</a>
                        <!--<button type="submit" class="login-button" name="submit">Log In</button>-->
                        <apex:commandButton action="{!login}" styleClass="login-button new-case" value="Log In" />
                    </div>
                </apex:form>
            </div>
            <a href="http://axcient.com/partners/#partner" title="Request User Portal Access" class="bottom-link">Request User Portal Access</a>
        </div>
