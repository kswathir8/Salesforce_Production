      <all>
        <total>All Responses</total>
        <qualified>Qualified</qualified>
        <sar>Sales Accepted</sar>
        <sqr>Sales Qualified</sqr>
        <closed>Closed Deals</closed>
        <!-- You can specify chart terms by funnel here.  -->
        <chart1>
          <total>Total Responses:</total>
        </chart1>
        <chart3>
          <total>Total Responses:</total>
        </chart3>
        <chart6>
          <total>Total Responses:</total>
        </chart6>
        <chart7>
          <total>Total Responses:</total>
        </chart7>
        <chart8>
          <total>Total Responses:</total>
