<?xml version="1.0" encoding="UTF-8"?>
<!-- Response intelligence taxonomy localization schema -->
<responseintelligence>
  <taxonomy> 
    Full Circle CRM Standard Taxonomy
  </taxonomy>
  <en-US> <!-- Default taxonomy English -->
